{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "symbol-art-parser",
  "version": "0.2.0",
  "description": "Phantasy Star Online 2 New Genesis Symbol Art file (*.sar) parser class.",
  "keywords": [
    "Symbol Art",
    "pso2",
    "pso2ngs",
    "ngs",
    "sa",
    "phantasy-star-online2"
  ],
  "license": "MIT",
  "type": "module",
  "author": {
    "name": "Logue",
    "email": "logue@hotmail.co.jp",
    "url": "https://logue.dev/"
  },
  "homepage": "https://github.com/logue/symbol-art-parser",
  "repository": {
    "type": "git",
    "url": "git@github.com:logue/symbol-art-parser.git"
  },
  "bugs": {
    "url": "https://github.com/logue/symbol-art-parser.js/issues"
  },
  "files": [
    "dist"
  ],
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "jsdelivr": "dist/index.iife.js",
  "unpkg": "dist/index.iife.js",
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "jsdelivr": "./dist/index.iife.js",
      "unpkg": "./dist/index.iife.js",
      "types": "./dist/src/index.d.ts"
    }
  },
  "engines": {
    "node": ">=18.12.5",
    "yarn": ">=1.22.19"
  },
  "packageManager": "yarn@4.0.2",
  "sideEffects": false,
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "clean": "rimraf node_modules/.vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:docs": "vite build --mode=docs",
    "build:clean": "rimraf dist",
    "build-only": "vite build",
    "type-check": "tsc --composite false",
    "lint": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint && prettier . --write",
    "preview": "vite preview --mode=docs",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@tsconfig/node-lts": "^20.1.0",
    "@tsconfig/strictest": "^2.0.2",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-json-schema-validator": "^4.7.4",
    "eslint-plugin-n": "^16.5.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-plugin-yaml": "^0.5.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-plugin-banner": "^0.7.1",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-dts": "^3.7.0",
    "vitest": "^1.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,json,yml,yaml,htm,html}": "eslint --fix --cache --cache-location ./node_modules/.vite/vite-plugin-eslint",
    "*": "prettier -w -u"
  },
  "resolutions": {
    "http-cache-semantics": "^4.1.1",
    "json5": "^2.2.3",
    "semver": "^7.5.2",
    "yaml": "^2.2.2"
  }
}
